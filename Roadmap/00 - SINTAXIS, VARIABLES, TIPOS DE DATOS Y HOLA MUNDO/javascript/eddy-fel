console.log(a);

a= 4; 
console.log(a);

let b = "hola mundo";
console.log(b);

b= 3; 
console.log(b);

/*const costante*/

const constante = "Hola soy una constante";
console.log(constante);

//datos primitivos 


//number 

0,1,2,3,4,5, .04, 0.2 

